        <FIELD name="number" type="text" label="COM_BALANCIRK_TABLE_TABLEHEAD_NUMBER" maxlength="10" />
        <FIELD name="bus" type="text" label="COM_BALANCIRK_TABLE_TABLEHEAD_BUS" maxlength="10" />
        <FIELD name="postcode" type="text" label="COM_BALANCIRK_TABLE_TABLEHEAD_POSTALCODE" maxlength="10" />
        <FIELD name="municipality" type="text" label="COM_BALANCIRK_TABLE_TABLEHEAD_MUNICIPALITY" maxlength="50" />
        <FIELD name="phone" type="text" label="COM_BALANCIRK_TABLE_TABLEHEAD_PHONE" maxlength="15" />
        <FIELD name="registerdate" type="calendar" label="COM_BALANCIRK_TABLE_TABLEHEAD_REGISTERDATE" readonly="true" format="%D %b %Y" />
